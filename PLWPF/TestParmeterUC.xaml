<UserControl x:Class="PLWPF.TestParmeterUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PLWPF"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="200">
    <Grid Margin="10,0,0,74">
        <CheckBox x:Name="PassedCB" Content="passed" HorizontalAlignment="Left"  VerticalAlignment="Top" Checked="PassedCB_Checked"/>
        <Label x:Name="ParmeterName" Content="" HorizontalAlignment="Left"  VerticalAlignment="Top" RenderTransformOrigin="-0.598,0.519"/>
        <CheckBox x:Name="FailCB" Content="falied" VerticalAlignment="Top" HorizontalAlignment="Right" Width="49" Checked="FailCB_Checked" Margin="0,0,70,0"/>

    </Grid>
</UserControl>
