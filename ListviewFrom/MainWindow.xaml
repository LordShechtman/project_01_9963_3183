<Window x:Class="ListviewFrom.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ListviewFrom"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="showTestBox" HorizontalAlignment="Left" Height="123" Margin="374,209,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="256" TextChanged="showTestBox_TextChanged"/>
        <Label x:Name="MainLabel" Content=" " HorizontalAlignment="Left" Margin="452,173,0,0" VerticalAlignment="Top" Width="77"/>
        <Label Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Label x:Name="problem" Content="Success!!" HorizontalAlignment="Left" Margin="226,209,0,0" VerticalAlignment="Top" Width="116" FontStyle="Italic"/>
        <Button Content="Testers" HorizontalAlignment="Left" Margin="498,374,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button x:Name="showTest" Content="Tests" HorizontalAlignment="Left" Margin="401,374,0,0" VerticalAlignment="Top" Width="75" Click="showTest_Click"/>
    </Grid>
</Window>
